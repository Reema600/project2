  <body>
    <% if (num <=0){%>
    <h2 style="background-color: rgb(253, 255, 192)">F</h2>

    <%} else if (num <=50){%>

    <h2 style="background-color: pink">B </h2>
    <%} else if (num <=75) {%>
       
    <h2 style="background-color: rgb(39, 7, 12)">C</h2>
    <%} else if (num <=100){%>
    <h2 style="background-color: rgb(192, 208, 255)">A</h2>
    <%} else {%>

    <h2 style="background-color: rgb(197, 255, 192)">Ther is no Result</h2>

    <%} %>
  </body>
